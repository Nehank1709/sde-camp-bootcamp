import java.util.*;

public class Factorial_10 {
    public static void main(String[] args) {
        
        Scanner scn = new Scanner(System.in);

        System.out.println("Enter value of n");  //taking input of n
        int n = scn.nextInt();

        long factorial = 1L;

        for(int i = 1; i <= n ; i++){  //calculating factorial
            factorial *= i;
        }

        System.out.println("The factorial of n is: " + factorial); //printing factorial

        scn.close();

    }
}
